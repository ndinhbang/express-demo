{
	"name": "express-typescript-app",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"exports": {
		".": "./dist/index.js",
		"./*": "./dist/*.js",
		"./package.json": "./package.json"
	},
	"main": "dist/index.js",
	"files": [
		"dist"
	],
	"scripts": {
		"lint": "NODE_ENV=development eslint .",
		"lint:prod": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write \"**/*.{md,y?(a)ml,json}\"",
		"dev": "NODE_ENV=development tsx watch --clear-screen=false src/start.ts",
		"build": "tsc --project tsconfig.prod.json && copyfiles \"src/**/*.{yaml,liquid}\" -u 1 dist"
	},
	"dependencies": {
		"cookie-parser": "~1.4.6",
		"debug": "~2.6.9",
		"dotenv": "^16.0.3",
		"express": "~4.18.2",
		"pg": "8.11.0"
	},
	"devDependencies": {
		"@types/debug": "^4.1.8",
		"@types/express": "^4.17.17",
		"@types/node": "^18.16.15",
		"@typescript-eslint/eslint-plugin": "^5.59.7",
		"@typescript-eslint/parser": "^5.59.7",
		"copyfiles": "^2.4.1",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"prettier": "^2.8.8",
		"tsx": "^3.12.7",
		"typescript": "^4.9.5"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}